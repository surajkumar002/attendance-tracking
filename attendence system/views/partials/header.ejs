    <meta name="robots" content="all,follow">
    <link rel = "icon" href = "<%= origin %>/static/images/logo2.png" type = "image/x-icon">

    <script type="text/javascript">
      function load_static_assets(exts, target) {
          console.log("Loading static assets...");
          var parent = document.getElementsByTagName(target)[0]; 
          exts.forEach((ext) => {
              var link = document.createElement('link');
              for(attr in ext) link[attr] = ext[attr]
              parent.appendChild(link);
          });
      }
    </script>

    <script>
        var dir = window.location.origin + "/static/";
        exts = [
            {
                tag: "link",
                href: "https://fonts.googleapis.com/css?family=Roboto:300,400,500,700",
                rel: "stylesheet",
                type: "text/css"
            },
            {
                tag: "link",
                href: dir + "vendor/choices.js/public/assets/styles/choices.min.css",
                rel: "stylesheet"
            },
            {
                tag: "link",
                href: dir + "vendor/overlayscrollbars/css/OverlayScrollbars.min.css",
                rel: "stylesheet"
            },
            {
                tag: "link",
                href: dir + "css/style.default.css",
                rel: "stylesheet",
                id: "theme-stylesheet"
            },
            {
                tag: "link",
                href: dir + "css/custom.css",
                rel: "stylesheet"
            },
            {
                tag: "link",
                href: dir + "img/favicon.ico",
                rel: "shortcut icon"
            },
        ];

        console.log(exts);
        load_static_assets(exts, 'head');
    </script>